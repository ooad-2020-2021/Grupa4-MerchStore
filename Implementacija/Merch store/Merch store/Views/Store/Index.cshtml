@model IEnumerable<Merch_store.Models.Store>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EmailNotifikacije)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UredajNotifikacije)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmailNotifikacije)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UredajNotifikacije)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Kod">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Kod">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Kod">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
